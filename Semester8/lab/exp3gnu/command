set terminal pdf
set output "resultRENAMEme.pdf"
g(x)=m*x + c
fit g(x) "dataLEDgreen" via m,c
set grid ytics lt 0 lw 1 lc rgb "#bbbbbb"
set grid xtics lt 0 lw 1 lc rgb "#bbbbbb"
set xlabel "Temperature T (Â°C)"
set ylabel "Voltage V (Volts)"
set title "LED green"
plot "dataLEDgreen" title "observations", g(x) title "linear fit"