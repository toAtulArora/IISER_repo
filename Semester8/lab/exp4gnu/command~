set terminal pdf
set output "resultRENAMEme.pdf"
g(x)=m*x + c
fit g(x) "dataLEDred" using 2:1 via m,c
set grid ytics lt 0 lw 1 lc rgb "#bbbbbb"
set grid xtics lt 0 lw 1 lc rgb "#bbbbbb"
set ylabel "Temperature T (Â°C)"
set xlabel "Voltage V (Volts)"
set title "LED red"
plot "dataLEDred" using 2:1 title "observations", g(x) title "linear fit"