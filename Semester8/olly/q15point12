>>> Mz=1
>>> s=(1.0/4)*(Mz*Mz)
>>> sW=0.23
>>> fz=s/((sW*sW)*(s-(Mz*Mz))
... )
>>> gv=-0.5 + 2*sW*sW
>>> ga=-0.5
>>> a1=2*fz*gv*gv + fz*fz*((gv*gv + ga*ga)**2)
>>> a2=4*fz*ga*ga + 8*fz*fz*gv*gv*ga*ga
>>> A=(3.0/8)*a2*a2*(1/(1+a1))
>>> A
2.456355232413752
>>> A=(3.0/8)*a2*(1/(1+a1))
>>> A
0.40677301489940987

