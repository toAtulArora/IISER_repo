\begin_layout Plain Layout
r vs d for Aluminium
\end_layout

\end_inset


\end_layout

\end_inset


\end_layout

\begin_layout Standard
aluminium
\end_layout

\begin_layout Standard
\begin_inset Formula $\log r=md+c$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $m=-1.91441\pm0.2667mm^{-1}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $c=6.19839\pm0.07119mm^{-1}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\mu=-m\approx1.92mm^{-1}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\mu_{m}\approx(19.2cm^{-1})/(2.7g/cm^{3})=7.1cm^{2}/g$
\end_inset


\end_layout

\begin_layout Standard
The quoted values are 
\begin_inset Formula $\mu=115.5cm^{-1}$
\end_inset

 and 
\begin_inset Formula $\mu_{m}=42.7cm^{2}/g$
\end_inset


\end_layout

